<table>
<thead>
<tr>
<th>ПІБ</th>
<th>Дисципліна</th>
<th>Семестр</th>
<th>Оцінка</th>
</tr>
</thead>
<tbody>
{% for row in rows %}
<tr>
    <td>{{ row.student_name }}</td>
    <td>{{ row.course_title }}</td>
    <td>{{ row.semester }}</td>
    <td>{{ row.points }}</td>
</tr>
{% endfor %}
</tbody>
</table>
